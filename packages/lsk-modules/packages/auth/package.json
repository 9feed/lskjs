{
  "name": "@lskjs/auth",
  "version": "0.0.1-alpha.0",
  "description": "LSK module for auth.",
  "author": "Igor Suvorov <hi@isuvorov.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/lskjs/modules/tree/master/packages/auth",
  "keywords": [
    "auth",
    "oAuth",
    "passport",
    "facebook",
    "telegram",
    "vk",
    "vkontakte"
  ],
  "dependencies": {
    "apiquery": "0.1.24",
    "bluebird": "3.5.1",
    "lodash": "^4.17.11",
    "passport": "0.4.0",
    "passport-facebook": "2.1.1",
    "passport-telegram-official": "1.0.3",
    "passport-vkontakte": "0.3.2",
    "passport-youtube-v3": "2.1.0",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "mobx": "5.6.0",
    "react-icons2": "2.4.0"
  },
  "peerDependencies": {
    "mobx": "*",
    "react-icons2": "*"
  },
  "scripts": {
    "release": "rm -rf dist && npm run babel-build && npm publish --access=public dist",
    "babel-build": "mkdir -p dist && cp {package.json,package-lock.json,README.md} dist/ && npx babel src --out-dir dist --source-maps inline --copy-files",
    "dev": "DEBUG=1 npm run babel-build -- --watch"
  }
}
