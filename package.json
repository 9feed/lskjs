    "__release": "npm publish",
    "__babel-build": "rimraf lib && babel src --out-dir lib --source-maps inline --copy-files && cp {package.json,README.md} lib/",
    "__deploy": "npm run babel-build && npm publish lib"
  },
  "scripts": {
    "release": "rm -rf dist && npm run babel-build && npm publish --access=public dist",
    "babel-build": "mkdir -p dist && cp {package.json,package-lock.json,README.md} dist/ && npx babel src --out-dir dist --source-maps inline --copy-files",
    "start": "node build/server.js",
    "dev": "DEBUG=1 npm run babel-build -- --watch",
    "test": "jest --config=test/config.json --watch",
    "storybook": "start-storybook -p 9001",
    "eslint": "eslint --ext .jsx,.js src --fix"
