  "scripts": {
    "build": "babel tools/run build --release",
    "start": "node build/server.js",
    "release": "npm publish",
    "test": "jest --config=test/config.json --watch",
    "dev": "node tools/run.js start | node ./src/utils/bunyan -o short -l trace",
    "dev5": "npx babel-node tools/run.js start  | node ./src/utils/bunyan -o short -l trace",
    "dev4": "npx babel-node tools/run.js",
    "dev2": "babel-node tools/run.js start  | node ./src/utils/bunyan -o short -l trace",
    "storybook": "start-storybook -p 9001",
    "babel-build": "rimraf lib && babel src --out-dir lib --source-maps inline --copy-files && cp {package.json,README.md} lib/",
    "deploy": "npm run babel-build && npm publish lib"
  }
